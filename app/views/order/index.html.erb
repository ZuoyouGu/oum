<h1>My orders</h1>

<table class="table">
  <tr class="row">
    <th>order id</th>
    <th>food id</th>
    <th>price</th>
    <th>quantity</th>
  </tr>
  <% @orders.each do |order|%>
      <tr class="row">
        <td><%= order.id %></td>
        <td><%= order.food_id %></td>
        <td><%= order.price %></td>
        <td><%= order.quantity %></td>
      </tr>
  <% end %>
</table>